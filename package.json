{
  "name": "@husam287/expo-template",
  "author": {
    "name": "Hossam Sherif",
    "email": "hossam.sherif.hassan@gmail.com",
    "url": "https://www.linkedin.com/in/hossamsherif28/"
  },
  "description": "This is expo react native start template which has most common packages",
  "keywords": [
    "React native",
    "expo",
    "sdk46",
    "redux",
    "redux-toolkit",
    "i18n",
    "absolute path",
    "start files",
    "template"
  ],
  "version": "1.0.9",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint --fix ./src/**/*.{js,jsx}",
    "deploy-test": "expo publish --release-channel testingv1",
    "deploy-live": "expo publish --release-channel prodv1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run lint",
      "git add ."
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.9",
    "@react-native-async-storage/async-storage": "^1.17.9",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@reduxjs/toolkit": "^1.8.4",
    "axios": "^0.27.2",
    "axios-hooks": "^4.0.0",
    "expo": "~46.0.6",
    "expo-localization": "~13.1.0",
    "expo-notifications": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.6",
    "i18n-js": "^4.1.0",
    "react": "18.0.0",
    "react-hook-form": "^7.37.0",
    "react-native": "0.69.6",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-redux": "^8.0.2",
    "yup": "^0.32.11",
    "expo-device": "~4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3"
  }
}