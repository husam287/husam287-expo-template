{
  "name": "@husam287/expo-template",
  "homepage": "https://github.com/husam287/husam287-expo-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/husam287/husam287-expo-template.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Hossam Sherif",
    "email": "hossam.sherif.hassan@gmail.com",
    "url": "https://www.linkedin.com/in/hossamsherif28/"
  },
  "description": "This is expo react native start template which has most common packages",
  "keywords": [
    "React native",
    "expo",
    "sdk48",
    "redux",
    "redux-toolkit",
    "Rtk query",
    "i18n",
    "absolute path",
    "start files",
    "template"
  ],
  "version": "1.1.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint --fix ./src/**/*.{js,jsx}",
    "deploy-test": "expo publish --release-channel testingv1",
    "deploy-live": "expo publish --release-channel prodv1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run lint",
      "git add ."
    ]
  },
