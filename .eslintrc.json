{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": [
        "airbnb",
        "plugin:react/recommended",
        "plugin:react-native/all",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "react/prop-types": 0,
        "import/no-extraneous-dependencies": 0,
        "react/jsx-props-no-spreading": 0,
        "react-native/no-raw-text": 0
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ]
            }
        }
    }
}