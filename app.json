{
  "expo": {
    "name": "husam287-expo-template",
    "slug": "husam287-expo-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "com.bit68.hossam",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bit68.hossam",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "com.bit68.hossam",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "locales": {
      "en": "./src/assets/i18n/en.json",
      "ar": "./src/assets/i18n/ar.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      "@mmomtchev/expo-android-localized-app-name"
    ]
  }
}
